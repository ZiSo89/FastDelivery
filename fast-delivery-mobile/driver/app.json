{
  "expo": {
    "name": "Driver Fast Delivery",
    "slug": "fast-delivery-driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fastdelivery.driver",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffffff"
      },
      "edgeToEdgeEnabled": false,
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#ffffff"
      },
      "navigationBar": {
        "backgroundColor": "#ffffff",
        "barStyle": "dark-content"
      },
      "package": "com.fastdelivery.driver",
      "permissions": [
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "POST_NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDUy3hiyc50qQv1ox6wyH4U9O_YsKyKdVE"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications",
      "expo-secure-store",
      "expo-asset",
      "expo-font",
      [
        "expo-system-ui",
        {
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7d7f4652-50aa-4515-8e0a-b2a83cc2abe9"
      }
    }
  }
}
