{
  "expo": {
    "name": "FastDelivery Driver",
    "slug": "fast-delivery-driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#00c2e8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fastdelivery.driver",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fastdelivery.driver",
      "permissions": [
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    },
    "plugins": [
      "expo-notifications",
      "expo-secure-store",
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "7d7f4652-50aa-4515-8e0a-b2a83cc2abe9"
      }
    }
  }
}
